{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --debug --config config/webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production API_BASE_URL=https://spinach-backend.herokuapp.com webpack --config config/webpack.prod.js --progress --profile --bail --verbose --colors --display-error-details",
    "clean:dist": "npm run rimraf -- dist",
    "lint": "eslint src/",
    "server:local": "cross-env API_BASE_URL=http://domain-name:8082 NODE_ENV=development webpack-dev-server --debug --output-pathinfo --watch --colors --inline --open",
    "server:dev": "cross-env API_BASE_URL=http://localhost:3333 NODE_ENV=development webpack-dev-server --debug --output-pathinfo --watch --colors --inline --open",
    "server:dev:tools": "cross-env API_BASE_URL=http://localhost:3333 NODE_ENV=development webpack-dev-server --debug --output-pathinfo --watch --colors --inline --open --env.devTools=true",
    "server:prod": "http-server ./dist",
    "start": "npm run server:dev",
    "heroku:start": "cross-env HEROKU=true npm run build:prod && npm run server:prod",
    "rimraf": "rimraf",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "bundle": " webpack --profile --json > stats/stats.json"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "es6",
    "babel",
    "webpack"
  ],
  "engines": {
    "node": "7.9.x",
    "npm": "4.2.x"
  },
  "dependencies": {
